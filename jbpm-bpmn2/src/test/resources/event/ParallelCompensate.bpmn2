             xmlns:tns="http://www.jboss.org/drools">

  <itemDefinition id="_priceItem" structureRef="Integer" />

  <process processType="Private" isExecutable="true" id="ParallelCompensate" name="Parallel Compensate Process" >

    <!-- process variables -->
    <property id="price" itemSubjectRef="_priceItem"/>

    <!-- nodes -->
    <startEvent id="_1" name="StartProcess" />
    <scriptTask id="_2" name="Reserve Order Items" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("Reserve Order Items");</script>
    </scriptTask>
    <endEvent id="_4" name="EndEvent" />
    <boundaryEvent id="_5" name="CompensateEvent" attachedToRef="_2" >
      <compensateEventDefinition/>
    </boundaryEvent>
    <scriptTask id="_6" name="Return Order Items" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("Compensating task");</script>
    </scriptTask>
    <endEvent id="_7" name="EndEvent" />
    <scriptTask id="_8" name="Approve Order" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("Approve Order");</script>
    </scriptTask>
    <parallelGateway id="_9" name="Gateway" gatewayDirection="Diverging" />
    <parallelGateway id="_10" name="Gateway" gatewayDirection="Converging" />
    <exclusiveGateway id="_11" name="Gateway" gatewayDirection="Diverging" />
    <endEvent id="_14" name="End" >
        <terminateEventDefinition />
    </endEvent>
    <scriptTask id="_15" name="Process Order" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("Process Order");</script>
    </scriptTask>

    <!-- connections -->
    <sequenceFlow id="_9-_2" sourceRef="_9" targetRef="_2" />
    <sequenceFlow id="_15-_4" sourceRef="_15" targetRef="_4" />
    <sequenceFlow id="_5-_6" sourceRef="_5" targetRef="_6" />
    <sequenceFlow id="_6-_7" sourceRef="_6" targetRef="_7" />
    <sequenceFlow id="_9-_8" sourceRef="_9" targetRef="_8" />
    <sequenceFlow id="_1-_9" sourceRef="_1" targetRef="_9" />
    <sequenceFlow id="_2-_10" sourceRef="_2" targetRef="_10" />
    <sequenceFlow id="_11-_10" sourceRef="_11" targetRef="_10" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.java.com/java" >
        return price &lt;= 100000;
      </conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_8-_11" sourceRef="_8" targetRef="_11" />
    <sequenceFlow id="_11-_14" sourceRef="_11" targetRef="_14" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.java.com/java" >
        return price &gt; 100000;
      </conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_10-_15" sourceRef="_10" targetRef="_15" />

  </process>

